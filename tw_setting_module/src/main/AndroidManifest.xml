<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.tw.tw_setting_module">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/TWSettingTheme">

        <activity android:name=".act.TwSettingActivity"
                  android:launchMode="singleTask">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".act.TwUserEditActivity"/>
        <activity android:name=".act.TwResetPasswordActivity"/>
        <activity android:name=".act.TwTitleActivity"/>
        <activity android:name=".act.TwAboutActivity"/>
        <activity android:name=".act.TwLogOutActivity"/>

    </application>

</manifest>